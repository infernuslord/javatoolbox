<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="1.0+"
    codebase="http://muggles.dyndns.org/javatoolbox/"
    href="$outputFile">
    
  <information>
    <title>Java Toolbox</title>
    <vendor>None</vendor>
    <homepage>http://code.google.com/p/javatoolbox</homepage>
    <description kind="one-line">Java Developers Toolbox</description>
    <offline-allowed/>
    <shortcut>
      <desktop/>
      <menu/>
    </shortcut>    
  </information>
  
  <security>
     <all-permissions/>
  </security>
  
  <update check="background" policy="prompt-update"/>
  
  <resources>
    <j2se version="1.5+"
     initial-heap-size="64m"
     max-heap-size="512m"/>
     $dependencies
  </resources>
  
  <application-desc main-class="$mainClass">
  </application-desc>
  
</jnlp>
